{
    "root": "../../",
    "version_bump": {
        "places": {
            "pyproject.toml": "version = \"<VERSION>\"",
            "build/build_depsland/src_max.json": "\"version\": \"<VERSION>\"",
            "depsland/__init__.py": "__version__ = '<VERSION>'"
        }
    },
    "images": {
        "src_max": "build/build_depsland/src_max.json",
        "src_min": "",
        "enc_max": "",
        "enc_min": ""
    },
    "encryption_options": {
        "key": "",
        "packages": [],
        "output": ""
    },
    "minideps_options": {
        "tree_shaking_model": {
            "search_paths": ["chore/.venv", "."],
            "entries": [
                "depsland/__init__.py",
                "depsland/__main__.py",
                "depsland/webui/app_manager/app.py",
                "depsland/webui/setup_wizard/app.py",
                "chore/.venv/streamlit/__init__.py",
                "chore/.venv/streamlit/__main__.py",
                "chore/.venv/streamlit_extras/__init__.py",
                "chore/.venv/toga_winforms/__init__.py"
            ],
            "ignores": [
                "ipython",
                "matplotlib",
                "numpy",
                "pandas",
                "pil",
                "plotly",
                "pyside6",
                "qmlease",
                "qtpy",
                "statsmodel",
                "statsmodels"
            ],
            "export": {
                "source": "chore/.venv",
                "target": "chore/site_packages"
            }
        }
    },
    "# post_script": "build/build_depsland/post_script.py",
    "post_script": ""
}
